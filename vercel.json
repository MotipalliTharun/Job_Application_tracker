{
  "version": 2,
  "buildCommand": "cd client && npm install && npm run build",
  "outputDirectory": "client/dist",
  "installCommand": "npm install && npm run install:all",
  "framework": null,
  "functions": {
    "api/applications.ts": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/applications"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

